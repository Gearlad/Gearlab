   "source": [
    "def tree(n):\n",
    "    for i in range(n+1):\n",
    "        print((' ' * (n - i)) + ('*' * (2*i-1)))\n",
    "    print((' ' * (n-1)) + '|')\n",
    "from ipywidgets import interact\n",
    "interact(tree, n=(1,50))"
